#include<stdio.h>
void read1(int arr[], int n){
	printf("Enter %d elements : ",n);
	for(int i = 0;i < n;i++){
		scanf("%d",&arr[i]);
	}
}
int linearSearch(int arr[], int start, int end, int key){
	if(start > end){
		return -1;
	}
	if (arr[start] == key) {
		return start;
	}
	return linearSearch(arr, start + 1, end, key);
}
